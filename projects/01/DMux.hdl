/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=sel, b=sel, out=nots);
    Nand(a=in, b=nots, out=c0);
    Nand(a=c0, b=c0, out=a);
    Nand(a=in, b=sel, out=c1);
    Nand(a=c1, b=c1, out=b);
}
