/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel, b=sel, out=nots);
    Nand(a=a, b=nots, out=c0);
    Nand(a=b, b=sel, out=c1);
    Nand(a=c0, b=c1, out=out);
}
